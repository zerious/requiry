{
  "name": "requiry",
  "version": "0.0.1",
  "description": "Package auto-installer for dev environments",
  "dependencies": {
    "execSync": "0.0.4",
    "plans": "^0.0.2"
  },
  "devDependencies": {
    "zeriousify": "^0.1.10",
    "exam": "^0.0.8",
    "istanbul": "0.2.11",
    "coveralls": "2.10.0"
  },
  "keywords": [
    "requiry"
  ],
  "main": "requiry.js",
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/requiry.git",
  "bugs": "https://github.com/lighterio/requiry/issues",
  "homepage": "http://github.com/lighterio/requiry",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    "Sam Eubank <sameubank@gmail.com>"
  ],
  "license": "MIT"
}
